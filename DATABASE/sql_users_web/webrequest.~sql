CREATE TABLE "COUNTERS"."WEBREQEST" 
   (	
   "WEBREQESTID" NUMBER(18,0) NOT NULL ENABLE, 
   "ID_BD" NUMBER(5,0) NOT NULL ENABLE, 
	"PROCESSED" NUMBER(1,0)  DEFAULT 0, 
	"CCURR" VARCHAR2(1) DEFAULT '0', 
	"CHOUR" VARCHAR2(1) DEFAULT '0', 
	"C24" VARCHAR2(1) DEFAULT '0', 
	"CSUM" VARCHAR2(1) DEFAULT '0', 
	"DFROMHOUR" DATE, 
	"DFROM24" DATE, 
 	"DTOHOUR" DATE, 
	"DTO24" DATE, 

	 CONSTRAINT "WEBREQEST_BDEV" FOREIGN KEY ("ID_BD")
	  REFERENCES "COUNTERS"."BDEVICES" ("ID_BD") ENABLE
   ) ;
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."ID_BD" IS '->BDEVICES';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."PROCESSED" IS '0 - в очереди  1 - идет опрос 2 - отработан';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."CCURR" IS '1 - опрашивать текущие';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."CHOUR" IS '1 - опрашивать часовые';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."C24" IS '1 - опрашивать суточные';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."CSUM" IS '1 - опрашивать итоговые';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."DFROMHOUR" IS 'Дата начала опроса часовых';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."DFROM24" IS 'Дата начала опроса  суточных';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."DTOHOUR" IS 'Дата завершения опроса часовых';
   COMMENT ON COLUMN "COUNTERS"."WEBREQEST"."DTO24" IS 'Дата завершения опроса суточных';
   COMMENT ON TABLE "COUNTERS"."WEBREQEST"  IS 'Запрос на опрос устройств';